{
    "name": "albinvar/github-telegram-bot",
    "description": "This is my package github-telegram-bot",
    "keywords": [
        "albinvar",
        "github-telegram-bot"
    ],
    "homepage": "https://github.com/albinvar/github-telegram-bot",
    "license": "MIT",
    "authors": [
        {
            "name": "Albin Varghese",
            "email": "albin8285@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "vlucas/phpdotenv": "^5.4",
        "ext-json": "*",
        "eleirbag89/telegrambotphp": "^1.3",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Albinvar\\GithubTelegramBot\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Albinvar\\GithubTelegramBot\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
